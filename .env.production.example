# MatterFlow Production Environment
# Copy to .env on your VPS and fill in values

# ============================================
# App Configuration
# ============================================
NODE_ENV=production
NEXT_PUBLIC_APP_URL=https://matter.develotype.com

# ============================================
# Supabase (from supabase.com dashboard)
# ============================================
NEXT_PUBLIC_SUPABASE_URL=https://xxxxx.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# ============================================
# Google OAuth (from Google Cloud Console)
# ============================================
GOOGLE_CLIENT_ID=xxxxx.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=xxxxx
GOOGLE_REDIRECT_URI=https://matter.develotype.com/api/auth/google/callback

# ============================================
# Square Payments (from Square Developer Dashboard)
# ============================================
SQUARE_ACCESS_TOKEN=xxxxx
SQUARE_ENVIRONMENT=production
SQUARE_LOCATION_ID=xxxxx
SQUARE_WEBHOOK_SIGNATURE_KEY=xxxxx

# ============================================
# Email (from resend.com)
# ============================================
RESEND_API_KEY=re_xxxxx
RESEND_FROM_EMAIL=MatterFlow <noreply@develotype.com>

# ============================================
# AI (from console.anthropic.com)
# ============================================
ANTHROPIC_API_KEY=sk-ant-xxxxx

# ============================================
# Cron Jobs
# ============================================
CRON_SECRET=generate-a-random-string-here
